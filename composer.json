{
    "name": "yaroslaw74/kyg-framework",
    "description": "KYG Framework for Business",
    "license": [
        "GPL-3.0-or-later",
        "MIT"
    ],
    "type": "project",
    "authors": [
        {
            "name": "Kataev Yaroslav",
            "email": "yaroslaw74@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "yaroslaw74@gmail.com",
        "issues": "https://github.com/yaroslaw74/kyg-framework/issues",
        "forum": "https://github.com/yaroslaw74/kyg-framework/discussions",
        "wiki": "https://github.com/yaroslaw74/kyg-framework/wiki",
        "source": "https://github.com/yaroslaw74/kyg-framework",
        "security": "https://github.com/yaroslaw74/kyg-framework/security"
    },
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "composer/composer": "^2.9.2",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6",
        "knplabs/knp-menu-bundle": "^3.7",
        "league/flysystem-bundle": "^3.6.1",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "7.4.*",
        "symfony/asset-mapper": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/doctrine-messenger": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10",
        "symfony/form": "7.4.*",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/http-kernel": "7.4.*",
        "symfony/intl": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/mime": "7.4.*",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/notifier": "7.4.*",
        "symfony/process": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/stopwatch": "7.4.*",
        "symfony/string": "7.4.*",
        "symfony/thanks": "^1.4.1",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/ux-icons": "^2.31",
        "symfony/ux-live-component": "^2.31",
        "symfony/ux-toggle-password": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/ux-twig-component": "^2.31",
        "symfony/validator": "7.4.*",
        "symfony/web-link": "7.4.*",
        "symfony/yaml": "7.4.*",
        "symfonycasts/sass-bundle": "^0.9.0",
        "twig/cssinliner-extra": "^3.22",
        "twig/extra-bundle": "^3.22.2",
        "twig/inky-extra": "^3.22",
        "twig/markdown-extra": "^3.22",
        "twig/string-extra": "^3.22.1",
        "twig/twig": "^3.22.2"
    },
    "require-dev": {
        "dadadev/phpstan-symfony-routing": "^0.2.0",
        "dama/doctrine-test-bundle": "^8.4.1",
        "ergebnis/composer-normalize": "^2.48.2",
        "friendsofphp/php-cs-fixer": "^3.92.3",
        "jawira/doctrine-diagram-bundle": "^1.2",
        "kocal/biome-js-bundle": "^2.1.2",
        "kocal/phpstan-symfony-ux": "^1.0",
        "neronmoon/scriptsdev": "^0.1.9",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-phpunit": "^2.0.11",
        "phpstan/phpstan-strict-rules": "^2.0.7",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^12.5.4",
        "shipmonk/composer-dependency-analyser": "^1.8.4",
        "spatie/symfony-ignition-bundle": "*",
        "symfony/browser-kit": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/web-profiler-bundle": "7.4.*",
        "symplify/config-transformer": "^12.4.3",
        "symplify/phpstan-extensions": "^12.0.2",
        "symplify/phpstan-rules": "^14.9.5"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Additions\\": "public/additions/",
            "App\\Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "neronmoon/scriptsdev": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": true
        },
        "apcu-autoloader": true,
        "bump-after-update": true,
        "optimize-autoloader": true,
        "process-timeout": 1200,
        "sort-packages": true
    },
    "extra": {
        "scripts-dev": {
            "post-install-cmd": [
                "@php bin/console biomejs:download"
            ],
            "post-update-cmd": [
                "@php bin/console doctrine:diagram:er",
                "@php bin/console doctrine:diagram:class",
                "vendor/bin/php-cs-fixer fix",
                "vendor/bin/parallel-lint --exclude .git --exclude vendor .",
                "vendor/bin/phpstan analyse",
                "bin/biome format --write assets modules"
            ]
        },
        "symfony": {
            "allow-contrib": "true",
            "require": "7.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
