{
    "name": "yaroslaw74/kyg-framework",
    "description": "KYG Framework for Business",
    "license": [
        "GPL-3.0-or-later",
        "MIT"
    ],
    "type": "project",
    "authors": [
        {
            "name": "Kataev Yaroslav",
            "email": "yaroslaw74@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "yaroslaw74@gmail.com",
        "issues": "https://github.com/yaroslaw74/kyg-framework/issues",
        "forum": "https://github.com/yaroslaw74/kyg-framework/discussions",
        "wiki": "https://github.com/yaroslaw74/kyg-framework/wiki",
        "source": "https://github.com/yaroslaw74/kyg-framework",
        "security": "https://github.com/yaroslaw74/kyg-framework/security"
    },
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "7.4.*",
        "symfony/asset-mapper": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/doctrine-messenger": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10",
        "symfony/form": "7.4.*",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/http-kernel": "7.4.*",
        "symfony/intl": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/mime": "7.4.*",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/notifier": "7.4.*",
        "symfony/process": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/stopwatch": "7.4.*",
        "symfony/string": "7.4.*",
        "symfony/thanks": "^1.4.1",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "7.4.*",
        "symfony/web-link": "7.4.*",
        "symfony/yaml": "7.4.*",
        "twig/cssinliner-extra": "^3.22",
        "twig/extra-bundle": "^3.22.2",
        "twig/inky-extra": "^3.22",
        "twig/markdown-extra": "^3.22",
        "twig/twig": "^3.22.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48",
        "phpunit/phpunit": "^12.5.4",
        "shipmonk/composer-dependency-analyser": "^1.8",
        "symfony/browser-kit": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Additions\\": "public/additions/",
            "App\\Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": true
        },
        "apcu-autoloader": true,
        "bump-after-update": true,
        "optimize-autoloader": true,
        "process-timeout": 1200,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
