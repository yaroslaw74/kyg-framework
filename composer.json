    "require-dev": {
        "dadadev/phpstan-symfony-routing": "^0.2",
        "dama/doctrine-test-bundle": "^8.6",
        "ergebnis/composer-normalize": "^2.49.0",
        "friendsofphp/php-cs-fixer": "^3.93.1",
        "gromnan/symfony-config-xml-to-php": "^0.2.1",
        "jawira/doctrine-diagram-bundle": "^1.2",
        "kocal/biome-js-bundle": "^2.1.2",
        "kocal/phpstan-symfony-ux": "^1.1",
        "matthiasnoback/symfony-config-test": "^6.1",
        "neronmoon/scriptsdev": "^0.1.9",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.37",
        "phpstan/phpstan-doctrine": "^2.0.14",
        "phpstan/phpstan-phpunit": "^2.0.12",
        "phpstan/phpstan-strict-rules": "^2.0.8",
        "phpstan/phpstan-symfony": "^2.0.12",
        "phpunit/phpunit": "^12.5.8",
        "psalm/plugin-symfony": "^5.2.8",
        "shipmonk/composer-dependency-analyser": "^1.8.4",
        "spatie/symfony-ignition-bundle": "^1.2",
        "symfony/browser-kit": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/dom-crawler": "7.4.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/web-profiler-bundle": "7.4.*",
        "symplify/config-transformer": "^12.4.3",
        "symplify/phpstan-extensions": "^12.0.2",
        "symplify/phpstan-rules": "^14.9.11",
        "vincentlanglet/twig-cs-fixer": "^3.13",
        "yamadashy/phpstan-friendly-formatter": "^1.4"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*",
        "symfony/polyfill-php85": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Additions\\": "public/additions/",
            "App\\Modules\\": "modules/"
        },
        "classmap": [
            "config/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "neronmoon/scriptsdev": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": true
        },
        "apcu-autoloader": true,
        "bump-after-update": true,
        "optimize-autoloader": true,
        "process-timeout": 3000,
        "sort-packages": true
    },
    "extra": {
        "copy-file": {
            "modules/Config/Resources/data/theme/": "public/theme/"
        },
        "mkdir": {
            "migrations": "0644",
            "public/additions": "0777",
            "public/storage": "0777",
            "public/storage/media": "0777",
            "public/translations": "0777",
            "public/uploads": "0777"
        },
        "scripts-dev": {
            "post-install-cmd": [
                "@php bin/console biomejs:download"
            ],
            "post-update-cmd": [
                "@php bin/console doctrine:diagram:er",
                "@php bin/console doctrine:diagram:class",
                "@php bin/console lint:twig templates/ public/additions/ modules/ --show-deprecations",
                "vendor/bin/twig-cs-fixer fix",
                "bin/biome lint --write assets modules public/additions",
                "bin/biome check --write assets modules public/additions",
                "bin/biome format --write assets modules public/additions",
                "vendor/bin/phpstan analyse --error-format friendly",
                "vendor/bin/psalm --alter --issues=MismatchingDocblockParamType,UnusedMethod --dry-run",
                "vendor/bin/php-cs-fixer fix"
            ]
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Balrok\\ComposerMkdir\\ScriptHandler::mkdirs",
            "SlowProg\\CopyFile\\ScriptHandler::copy",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "requirements-checker": "script",
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "ckeditor:install --clear=drop --release=full": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "kygbundle:install --symlink --relative": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "flasher:install": "symfony-cmd"
        }
    }
}
