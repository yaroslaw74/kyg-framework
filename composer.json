    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Additions\\": "public/additions/",
            "App\\Modules\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "neronmoon/scriptsdev": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "symfony/thanks": true
        },
        "apcu-autoloader": true,
        "bump-after-update": true,
        "optimize-autoloader": true,
        "process-timeout": 3000,
        "sort-packages": true
    },
    "extra": {
        "copy-file": {
            "modules/Install/Resources/data/images/": "public/theme/images/",
            "modules/Install/Resources/data/config/": "var/config/"
        },
        "mkdir": {
            "migrations": "0644",
            "public/additions": "0777",
            "public/storage": "0777",
            "public/translations": "0777",
            "public/uploads": "0777"
        },
        "scripts-dev": {
            "post-install-cmd": [
                "@php bin/console biomejs:download"
            ],
            "post-update-cmd": [
                "@php bin/console doctrine:diagram:er",
                "@php bin/console doctrine:diagram:class",
                "vendor/bin/php-cs-fixer fix",
                "vendor/bin/twig-cs-fixer fix",
                "vendor/bin/phpstan analyse --error-format friendly",
                "bin/biome format --write assets modules"
            ]
        },
        "symfony": {
            "allow-contrib": "true",
            "require": "7.4.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Balrok\\ComposerMkdir\\ScriptHandler::mkdirs",
            "SlowProg\\CopyFile\\ScriptHandler::copy",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "requirements-checker": "script",
            "cache:clear": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "ckeditor:install --clear=drop --release=full": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "sass:build": "symfony-cmd",
            "asset-map:compile": "symfony-cmd"
        }
    }
}
